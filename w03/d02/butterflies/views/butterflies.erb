<% @rows.each do |row| %>
  <section>
    <form action='/butterflies/<%= row['id'] %>/delete' method='post'>
      <button>Delete</button>
    </form>

    <a href='/butterflies/<%= row['id'] %>/edit/'>Edit</a>

    <%= row['name'] %>
    <%= row['family'] %>
    <img src='<%= row['photo'] %>'>
  </section>
<% end %>
